{
  "presets": [
    [ "env", {
      "modules": false,
      "exclude": [ "transform-regenerator", "transform-async-to-generator" ],
      "targets": {
        "browsers": [
          "last 2 versions",
          "ie >= 11"
        ]
      }
    }],
    "react"
  ],
  "plugins": [
    "relay",
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-do-expressions",
    "transform-object-rest-spread"
  ]
}